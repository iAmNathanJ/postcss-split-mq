{
  "name": "postcss-split-mq",
  "version": "1.3.0",
  "description": "Postcss plugin to split media queries into separate files",
  "repository": {
    "type": "git",
    "url": "https://github.com/iAmNathanJ/postcss-split-mq"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "babel src --out-dir dist -s inline",
    "test": "xo && ava",
    "dev": "run-p \"build -- -w\" \"test -- -w\"",
    "ci": "nyc --reporter=lcov ava",
    "prepublish": "run-s build test"
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/*.js"
    ]
  },
  "xo": {
    "space": true,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "array-bracket-spacing": 0
    }
  }, 
  "keywords": [
    "postcss-plugin",
    "postcss",
    "plugin",
    "media",
    "query",
    "queries",
    "split",
    "performance"
  ],
  "author": "Nathan Jacobs",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "shelljs": "^0.7.8",
    "tempy": "^0.2.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "postcss": "^6.0.13",
    "postcss-media-query-parser": "^0.2.3",
    "postcss-unwrap-helper": "^0.1.0"
  },
  "engines": {
    "node": ">=6.10"
  }
}
